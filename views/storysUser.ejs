<!DOCTYPE html>
<html lang="en">

<%- include("./comps/head") %>

<body>

  <%-include("./comps/navbarTerciary")%>
  <div class="container-titulo-section-stories-user">
    <h4>Historias de <%= stories[0].name %></h4>
    <div class="container-img-perfil">
      <img src="<%= stories[0].image %>" alt="">
    </div>

  </div>
  <div class="row container-stories-author">
    <% stories.forEach(story => { %>
    <div class="col s12 m4">
      <div class="card">
        <div class=" card-image">
        </div>
        <div class="card-content center-align container-story">
          <h5> <%=story.title%> </h5>
          <p class="body-story"> <%= story.body %> </p>
          <br>
        </div>
        <div class="card-action center-align">
          <a href="/stories/<%= story._id %>" class="btn grey">Leer m√°s...</a>
        </div>
      </div>
    </div>
    <% }) %>
  </div>

  <%- include("./comps/scriptsMat.ejs") %>
</body>

</html>